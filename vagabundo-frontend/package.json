{
  "name": "vagabundo-frontend",
  "version": "0.0.6",
  "config": {
    "api": "1"
  },
  "scripts": {
    "clean": "rimraf api/* && rimraf dist/* && rimraf dist-staging/*",
    "load-api": "node etc/load-api.ts",
    "prestart": "npm run clean && npm run load-api",
    "start": "ng-swagger-gen && ng serve --proxy-config etc/proxy.conf.js --open",
    "prebuild": "npm run clean && npm run load-api",
    "build": "ng-swagger-gen && ng build --configuration=production --no-progress",
    "build:staging": "ng-swagger-gen && ng build --configuration=staging --no-progress",
    "test": "ng test --watch",
    "test:prod": "ng test --no-watch --no-progress --browsers=HeadlessChrome",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "upload": "node aws-s3.ts",
    "bundle-report": "ng build --prod --stats-json && webpack-bundle-analyzer dist/vagabundo-frontend/stats.json"
  },
  "dependencies": {
    "@angular-devkit/build-optimizer": "^0.13.8",
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "@auth0/angular-jwt": "^2.1.0",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.14",
    "@fortawesome/free-solid-svg-icons": "^5.7.1",
    "@ng-bootstrap/ng-bootstrap": "^4.0.1",
    "@ngrx/effects": "^7.2.0",
    "@ngrx/router-store": "^7.2.0",
    "@ngrx/store": "^7.2.0",
    "@ngrx/store-devtools": "^7.2.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "d3": "^5.8.2",
    "dayjs": "^1.8.12",
    "ngx-cookie-service": "^2.1.0",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.9",
    "@angular/cli": "^7.3.9",
    "@angular/compiler-cli": "^7.2.15",
    "@angular/language-service": "~7.0.0",
    "@ngrx/schematics": "^7.2.0",
    "@types/d3": "^5.7.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.9.5",
    "aws-sdk": "^2.388.0",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^6.3.16",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mkdirp": "^0.5.1",
    "ng-swagger-gen": "^1.4.3",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
