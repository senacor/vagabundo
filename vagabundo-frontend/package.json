{
  "name": "vagabundo-frontend",
  "version": "0.0.6",
  "config": {
    "api": "1"
  },
  "scripts": {
    "clean": "rimraf api/* && rimraf dist/* && rimraf dist-staging/*",
    "load-api": "node etc/load-api.ts",
    "prestart": "npm run clean && npm run load-api",
    "start": "ng-swagger-gen && ng serve --proxy-config etc/proxy.conf.js --open",
    "prebuild": "npm run clean && npm run load-api",
    "build": "ng-swagger-gen && ng build --configuration=production --no-progress",
    "build:staging": "ng-swagger-gen && ng build --configuration=staging --no-progress",
    "test": "ng test --watch",
    "test:prod": "ng test --no-watch --no-progress --browsers=HeadlessChrome",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "upload": "node aws-s3.ts",
    "bundle-report": "ng build --prod --stats-json && webpack-bundle-analyzer dist/vagabundo-frontend/stats.json"
  },
  "dependencies": {
    "@angular-devkit/build-optimizer": "^0.13.8",
    "@angular/animations": "~13.3.0",
    "@angular/cdk": "^13.3.3",
    "@angular/common": "~13.3.11",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "~13.3.0",
    "@angular/http": "5.2.11",
    "@angular/material": "^13.3.3",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@auth0/angular-jwt": "4.2.0",
    "@fortawesome/angular-fontawesome": "^0.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.14",
    "@fortawesome/free-solid-svg-icons": "^5.7.1",
    "@ng-bootstrap/ng-bootstrap": "12.1.2",
    "@ngrx/effects": "^7.2.0",
    "@ngrx/router-store": "^7.2.0",
    "@ngrx/store": "^7.2.0",
    "@ngrx/store-devtools": "^7.2.0",
    "bootstrap": "5.2.0-beta1",
    "core-js": "^2.5.4",
    "d3": "^5.8.2",
    "dayjs": "^1.8.12",
    "ngx-cookie-service": "^2.1.0",
    "node-sass": "^7.0.1",
    "package": "^1.0.1",
    "rxjs": "~7.5.0",
    "watch": "^1.0.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.3",
    "@angular/cli": "~13.3.3",
    "@angular/compiler-cli": "~13.3.0",
    "@angular/language-service": "~7.0.0",
    "@ngrx/schematics": "^7.2.0",
    "@types/d3": "^5.7.0",
    "@types/jasmine": "~3.10.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "aws-sdk": "^2.1163.0",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~4.0.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "4.0.2",
    "karma-jasmine-html-reporter": "~1.7.0",
    "mkdirp": "^0.5.1",
    "ng-swagger-gen": "^1.8.1",
    "protractor": "7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~4.6.2",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
